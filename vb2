Public Class Form1
    Private Sub btn_02_Click(sender As Object, e As EventArgs) Handles btn_02.Click
        Dim m0, m1, m2, m3
        FileOpen(1, "C:\Users\user\test\1060302.SM", OpenMode.Input)
        Input(1, m0)
        FileClose(1)
        For i = 1 To m0
            For j = 1 To i
                m1 = m1 & j
                txt_02.Text = m1
            Next
            m1 = m1 & vbCrLf
        Next


    End Sub
End Class
